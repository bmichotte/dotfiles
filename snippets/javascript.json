{
	"Query Selector": {
		"prefix": "ds",
		"body": ["document.querySelector('$1')"]
	},
	"Console Log": {
		"prefix": "con",
		"body": ["console.log($1)"]
	},
	"Author": {
		"prefix": "@author",
		"body": ["@author Benjamin Michotte <benjamin@opp.mx>"]
	},
	"useState snippet": {
		"prefix": "rus",
		"body": "const [${1}, set${1/(^[a-zA-Z])(.*)/${1:/upcase}${2}/}] = useState(${2:default${1/(^[a-zA-Z])(.*)/${1:/upcase}${2}/}});",
		"description": "use state but it camel cases"
	}
}
